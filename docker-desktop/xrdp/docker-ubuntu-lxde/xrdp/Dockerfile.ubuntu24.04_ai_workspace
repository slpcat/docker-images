#kubectl-ai
RUN \
    wget https://github.com/GoogleCloudPlatform/kubectl-ai/releases/download/v0.0.29/kubectl-ai_Linux_x86_64.tar.gz \
    && tar -xzf kubectl-ai_Linux_x86_64.tar.gz \
    && chmod +x kubectl-ai \
    && mv kubectl-ai /usr/local/bin \
    && rm -f kubectl-ai_Linux_x86_64.tar.gz LICENSE README.md

#k8sgpt
RUN \
    wget https://github.com/k8sgpt-ai/k8sgpt/releases/download/v0.4.27/k8sgpt_amd64.deb \
    && apt-get install -y ./k8sgpt_amd64.deb && rm -f k8sgpt_amd64.deb

# Clippy lets you run a variety of large language models (LLMs) locally on your computer while sticking with a user interface of the 1990s
# https://felixrieseberg.github.io/clippy/
RUN \
    wget https://github.com/felixrieseberg/clippy/releases/download/v0.4.3/clippy_0.4.3_amd64.deb \
    && apt-get install -y ./clippy_0.4.3_amd64.deb && rm -f clippy_0.4.3_amd64.deb

