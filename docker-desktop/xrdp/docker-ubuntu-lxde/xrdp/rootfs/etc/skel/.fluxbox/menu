        [workspaces] (Workspaces)
        [separator]
        [reconfig] (Reconfigure)
        [restart] (Restart)
    [end]
    [exec] (Exit) {desktop-session-exit} </usr/share/icons/papirus-antix/48x48/apps/application-exit.png>
